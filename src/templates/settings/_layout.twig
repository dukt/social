{% extends "_layouts/cp" %}

{% set title = "Social Settings"|t('social') %}

{% set docsUrl = 'https://dukt.net/docs/social/v2/' %}

{% set crumbs = [
    { label: "Settings"|t('social'), url: url('settings') },
] %}

{% set navItems = {
    loginProviders: { label: "Login Providers"|t('social'), url: url('settings/social/loginproviders') },
    settings: { label: "Settings"|t('social'), url: url('settings/social/settings') },
} %}

{% set docTitle = navItems[selectedNavItem].label~' - '~title %}

{% block sidebar %}
    {% include "_includes/nav" with { items: navItems, selectedItem: selectedNavItem } only %}
{% endblock %}
